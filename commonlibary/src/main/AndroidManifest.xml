<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.commonlibary">

    <application android:allowBackup="true" android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".pickerview.ComonApplication"
       >
     <activity android:name=".pickerview.takephoto.CameraActivity"
         android:configChanges="orientation|keyboard|keyboardHidden|navigation"
         android:screenOrientation="nosensor"
         android:theme="@android:style/Theme.Translucent"/>
        <activity android:name=".pickerview.takephoto.activity.PhotoViewActivity"
            android:configChanges="orientation|keyboard|keyboardHidden|navigation"
            android:screenOrientation="nosensor"/>

        <activity android:name=".pickerview.takephoto.ChoosePhotoGridActivity"
            android:configChanges="orientation|keyboard|keyboardHidden|navigation"
            android:screenOrientation="nosensor"/>
        <!--图片浏览器 -->
        <activity android:name=".pickerview.takephoto.activity.ShowPhotoActivity"
            android:configChanges="orientation|keyboard|keyboardHidden|navigation"
            android:screenOrientation="nosensor"
        />
    </application>

</manifest>